{
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "extends": [
    "eslint:all",
    "plugin:react/all"
  ],
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "rules": {
    "react/no-multi-comp": [1, {"ignoreStateless": false}],
    "react/prefer-es6-class": [1, "always"],
    "react/prefer-stateless-function": 0,
    "react/jsx-pascal-case": [1, {"allowAllCaps": false}],
    "react/jsx-closing-bracket-location": [1, "tag-aligned"],
    "react/jsx-tag-spacing": [1, {
      "closingSlash": "never",
      "beforeSelfClosing": "always",
      "afterOpening": "never"
    }],
    "react/jsx-curly-spacing": [1, {"when": "never", "spacing": {"objectLiterals": "never"}}],
    "react/jsx-boolean-value": [1, "never"],
    "react/no-string-refs": 1,
    "react/jsx-wrap-multilines": [1, {
      "declaration": true,
      "assignment": true,
      "return": true,
      "arrow": true
    }],
    "react/self-closing-comp": [1, {
      "component": true,
      "html": true
    }],
    "react/jsx-no-bind": [1, {
      "ignoreRefs": true,
      "allowArrowFunctions": true,
      "allowBind": false
    }],
    "react/require-render-return": 1,
    "react/sort-comp": [1, {
      "order": [
        "static-methods",
        "lifecycle",
        "everything-else",
        "render"
      ],
      "groups": {
        "lifecycle": [
          "displayName",
          "propTypes",
          "contextTypes",
          "childContextTypes",
          "mixins",
          "statics",
          "defaultProps",
          "constructor",
          "getDefaultProps",
          "getInitialState",
          "state",
          "getChildContext",
          "componentWillMount",
          "componentDidMount",
          "componentWillReceiveProps",
          "shouldComponentUpdate",
          "componentWillUpdate",
          "componentDidUpdate",
          "componentWillUnmount"
        ]
      }
    }],
    "react/forbid-component-props": 0,
    "react/forbid-prop-types": 0,
    "react/no-is-mounted": 1,
    "react/no-set-state": 0,
    "react/no-unused-prop-types": 0,
    "react/jsx-max-props-per-line": 0,
    "react/jsx-handler-names": 0,
    "react/jsx-no-literals": 0,
    "react/require-optimization": 0,
    "react/require-default-props": 0,
    "react/no-unused-state": 0,
    "valid-jsdoc": 0,
    "prefer-const": 0,
    "padded-blocks": [1, {"blocks": "never", "classes": "always"}],
    "no-magic-numbers": 0,
    "sort-keys": 0,
    "max-lines": 0,
    "jsx-quotes": [1, "prefer-double"],
    "no-multi-spaces": [1, {"ignoreEOLComments": true}],
    "sort-vars": 0,
    "sort-imports":0,
    "object-curly-newline": [1, {
      "ObjectExpression": "always",
      "ObjectPattern": { "multiline": true }
    }],
    "max-len": 0,
    "dot-location": [1, "property"],
    "no-console": 0,
    "arrow-body-style": [1, "always"],
    "semi": [1, "always"],
    "radix": [1, "as-needed"],
    "line-comment-position": 0,
    "no-inline-comments": 0,
    "no-prototype-builtins": 1,
    "object-shorthand": [1, "consistent"],
    "class-methods-use-this": 0,
    "no-extra-parens": 0,
    "indent": 0
  }
}